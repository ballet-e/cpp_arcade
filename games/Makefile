all:
	make -C ./libsnake/
	make -C ./libsolarfox/
	make -C ./libsokoban/
	make -C ./libwolf3d/

clean:
	make -C ./libsnake/ clean
	make -C ./libsolarfox/ clean
	make -C ./libsokoban/ clean
	make -C ./libwolf3d/ clean

fclean:
	make -C ./libsnake/ fclean
	make -C ./libsolarfox/ fclean
	make -C ./libsokoban/ fclean
	make -C ./libwolf3d/ fclean

re:	fclean all
